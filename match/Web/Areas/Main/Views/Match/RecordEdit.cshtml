@using Model;
@using BLL;
@using Web;

@model tblteams

@{
    ViewBag.Title = "";
    Layout = "~/Views/Shared/_Layout.Edit.cshtml";
}

@section PageSpecificJavascriptIncludes{

    <script type="text/javascript">
        $(document).ready(function () {
            $("#mainForm").submit(function () {

                return true;
            });
        });
    </script>
}

@section MainContent{
    <div class="portlet-body form-horizontal form-bordered form-row-stripped">
        <div class="row-fluid">
            <div id="msg">@Html.Raw(ViewBag.ErrorMsg)</div>

            <div class="control-group">
                <label class="control-label"><span class="required">*</span>当前标识点：</label>
                <div class="controls">
                    <select id="optPoint" name="optPoint" data-placeholder="请选择标识点" class="m-wrap large">
                        @Html.Raw(ViewBag.points)
                    </select>
                </div>
            </div>
        </div>
    </div>
}
<script type="text/javascript" src="@Url.Content("~/scripts/js/selector/jquery-1.10.js")"></script>

