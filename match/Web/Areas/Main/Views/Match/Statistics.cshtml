@using Utls;
@using Model;
@using Web;
@{
    ViewBag.Title = "";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model Statistics
<div class="row-fluid">
    <div class="span3" style="text-align:left">
        <div>
            参赛的队伍：
        </div>
    </div>
    <div class="span9" style="text-align:left">
        <div>
            @Model.totalteam
        </div>
    </div>
</div>
<div class="row-fluid">
    <div class="span3" style="text-align:left">
        <div>
            已检录的队伍：
        </div>
    </div>
    <div class="span9" style="text-align:left">
        <div>
            @Model.checkedteam
        </div>
    </div>
</div>
<div class="row-fluid">
    <div class="span12" style="text-align: left; background-color: aqua">
        <div>
            未检录的队伍：【<a href="#" onclick="ExpNoCheckTeam();">下载详细</a>】
        </div>
    </div>
</div>

 @if (Model.nocheckedteam != null)
{
    foreach (var m in Model.nocheckedteam)
    {
        <div class="row-fluid">
            <div class="span3" style="text-align:left">
                <div>
                    @m.linesname
                </div>
            </div>
            <div class="span9" style="text-align: left">
                <div>
                    @m.count
                </div>
            </div>
        </div>
    }
}

<div class="row-fluid">
    <div class="span12" style="text-align:left;background-color:aqua">
        <div>
            各线完成队伍：【<a href="#" onclick="ExpFinishTeam();">下载详细</a>】 </div>
    </div>
</div>

@if (Model.linesfinishteam != null)
{
    foreach (var m in Model.linesfinishteam)
    {
        <div class="row-fluid">
            <div class="span3" style="text-align:left">
                <div>
                    @m.linesname
                </div>
            </div>
            <div class="span9" style="text-align: left">
                <div>
                    @m.count
                </div>
            </div>
        </div>
    }
}


<script type="text/javascript">
    function ExpNoCheckTeam() {
        var url = "GetNoCheckTeamResult";
        window.open(url);
    }
    function ExpFinishTeam() {
        var url = "GetFinishTeamResult";
        window.open(url);
    }
</script>